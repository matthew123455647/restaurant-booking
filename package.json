{
  "name": "restaurant-booking",
  "version": "1.0.0",
  "description": "This is a project for DVOPs",
  "main": "index.js",
  "nyc": {
    "include": [
      "utils/RestaurantUtil.js",
      "utils/CommentUtil.js",
      "utils/AddCommentUtil.js",
      "utils/AddBookingUtil.js",
      "utils/DeleteBookingUtil.js",
      "utils/ViewBookingUtil.js",
      "utils/LoginUtil.js",
      "utils/UserUtil.js",
      "index.js",
      "public/**/*.js"
    ],
    "exclude": [],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "scripts": {
    "test": "nyc mocha tests/*.test.js",
    "start": "node index.js",
    "frontend-instrument": "nyc instrument .\\public\\js .\\public\\instrumented\\js && copy .\\public\\*.html .\\public\\instrumented\\ && copy .\\public\\images .\\public\\instrumented\\ && copy .\\public\\css .\\public\\instrumented\\ && del .\\coverage-frontend\\*.json",
    "frontend-report": "nyc report --reporter html --reporter text -t coverage-frontend --report-dir coverage-frontend/summary"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.4.0",
    "chromedriver": "^120.0.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "selenium-webdriver": "^4.16.0"
  },
  "devDependencies": {
    "sinon": "^17.0.1"
  }
}
